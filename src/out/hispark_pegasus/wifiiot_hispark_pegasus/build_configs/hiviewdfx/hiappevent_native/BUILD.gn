ohos_part("hiappevent_native") {
  subsystem_name = "hiviewdfx"
  module_list = [
    "//base/hiviewdfx/hiappevent/frameworks/native/libhiappevent:libhiappevent_base",
    "//base/hiviewdfx/hiappevent/frameworks/native/ndk:hiappevent_ndk",
