ohos_part_test("hiappevent_native_test") {
  testonly = true
  test_packages = [ "//base/hiviewdfx/hiappevent/test:unittest" ]
  deps = [ ":hiappevent_native" ]
  part_name = "hiappevent_native"
  subsystem_name = "hiviewdfx"
}
